        </dependency>

        <!--jwt的sdk-->
        <dependency>
            <groupId>x.trident</groupId>
            <artifactId>jwt-sdk</artifactId>
            <version>${trident.version}</version>
        </dependency>

    </dependencies>

    <build>
        <finalName>${project.artifactId}</finalName>
    </build>

</project>
